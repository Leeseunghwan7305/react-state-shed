{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react", // .tsx 파일 처리 설정 추가
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node" // React 모듈 해석 방식 설정
  },
  "include": ["./stan/src/**/*"],
  "exclude": ["dist", "node_modules"]
}
